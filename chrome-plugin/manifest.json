{
  "manifest_version": 2,
  "name": "VCS integration",
  "version": "1.0",
  "icons": {
    "16": "16.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "js": [
      "jquery.min.js",
      "bitbucket.js"
    ],
    "matches": ["https://bitbucket.org/*/pull-requests/*"]
  }],
  "browser_action": {
    "default_title": "VCS integration",
    "default_icon": "64.png"
  },
  "options_page": "options.html"
}